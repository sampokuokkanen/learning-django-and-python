[Unit]
Description=Daphne Service for DOMAIN
After=network.target

[Service]
Restart=on-failure
User=sam
WorkingDirectory=/home/sam/sites/DOMAIN
EnvironmentFile=/home/sam/sites/DOMAIN/.env

ExecStart=/home/sam/sites/DOMAIN/virtualenv/bin/daphne -b 0.0.0.0 -p 9001 superlists.asgi:application
[Install]
WantedBy=multi-user.target